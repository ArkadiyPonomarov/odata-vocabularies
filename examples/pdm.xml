  <edmx:Reference Uri="https://wiki.scn.sap.com/wiki/download/attachments/496435637/PersonalData.xml?api=v2" xmlns:edmx="http://docs.oasis-open.org/odata/ns/edmx">
    <edmx:Include  Alias="PersonalData" Namespace="com.sap.vocabularies.PersonalData.v1"/>
  </edmx:Reference>
  <edmx:Reference Uri="https://wiki.scn.sap.com/wiki/download/attachments/448470968/UI.xml?api=v2" xmlns:edmx="http://docs.oasis-open.org/odata/ns/edmx">
    <edmx:Include Alias="UI" Namespace="com.sap.vocabularies.UI.v1"/>
  </edmx:Reference>
  <edmx:DataServices m:DataServiceVersion="2.0">
    <Schema Namespace="gdpr.gra.sample" xmlns="http://schemas.microsoft.com/ado/2008/09/edm">
      <EntityContainer Name="EntityContainer" m:IsDefaultEntityContainer="true">
        <EntitySet Name="Employee" EntityType="gdpr.gra.sample.Employee"/>
        <EntitySet Name="Address" EntityType="gdpr.gra.sample.Address"/>
        <EntitySet Name="LeaveRequest" EntityType="gdpr.gra.sample.LeaveRequest"/>
        <AssociationSet Name="Address_employee" Association="gdpr.gra.sample.Address_employee">
          <End Role="Address" EntitySet="Address"/>
          <End Role="Employee" EntitySet="Employee"/>
        </AssociationSet>
        <AssociationSet Name="LeaveRequest_employee" Association="gdpr.gra.sample.LeaveRequest_employee">
          <End Role="LeaveRequest" EntitySet="LeaveRequest"/>
          <End Role="Employee" EntitySet="Employee"/>
        </AssociationSet>
      </EntityContainer>

      <EntityType Name="Employee">
        <Key>
          <PropertyRef Name="userId"/>
        </Key>
        <Property Name="userId" Type="Edm.String" MaxLength="16" Nullable="false"/>
        <Property Name="email" Type="Edm.String" MaxLength="256"/>
        <Property Name="firstName" Type="Edm.String" MaxLength="256" Nullable="false"/>
        <Property Name="middleName" Type="Edm.String" MaxLength="256"/>
        <Property Name="lastName" Type="Edm.String" MaxLength="256" Nullable="false"/>
        <Property Name="genderCode" Type="Edm.String" MaxLength="1"/>
        <Property Name="birthDate" Type="Edm.DateTime" sap:display-format="Date"/>
        <NavigationProperty Name="addresses" Relationship="gdpr.gra.sample.Address_employee" FromRole="Employee" ToRole="Address"/>
        <NavigationProperty Name="leaveRequests" Relationship="gdpr.gra.sample.LeaveRequest_employee" FromRole="Employee" ToRole="LeaveRequest"/>
      </EntityType>

      <EntityType Name="Address">
        <Key>
          <PropertyRef Name="addressId"/>
        </Key>
        <Property Name="addressId" Type="Edm.String" Nullable="false"/>
        <Property Name="country" Type="Edm.String" MaxLength="256"/>
        <Property Name="region" Type="Edm.String" MaxLength="256"/>
        <Property Name="city" Type="Edm.String" MaxLength="256"/>
        <Property Name="district" Type="Edm.String" MaxLength="256"/>
        <Property Name="streetPostalCode" Type="Edm.String" MaxLength="10"/>
        <Property Name="streetName" Type="Edm.String" MaxLength="60"/>
        <Property Name="houseID" Type="Edm.String" MaxLength="10"/>
        <Property Name="careOfName" Type="Edm.String" MaxLength="40"/>
        <Property Name="buildingID" Type="Edm.String" MaxLength="10"/>
        <NavigationProperty Name="employee" Relationship="gdpr.gra.sample.Address_employee" FromRole="Address" ToRole="Employee"/>
        <Property Name="employee_userId" Type="Edm.String" MaxLength="16" Nullable="false"/>
      </EntityType>

      <EntityType Name="LeaveRequest">
        <Key>
          <PropertyRef Name="leaveRequestId"/>
        </Key>
        <Property Name="leaveRequestId" Type="Edm.Int64" Nullable="false"/>
        <Property Name="leaveType" Type="Edm.String" MaxLength="32" Nullable="false"/>
        <Property Name="leaveStartDate" Type="Edm.DateTime" Nullable="false" sap:display-format="Date"/>
        <Property Name="leaveEndDate" Type="Edm.DateTime" Nullable="false" sap:display-format="Date"/>
        <Property Name="leaveReason" Type="Edm.String" MaxLength="1024"/>
        <Property Name="leaveRequestStatus" Type="Edm.String" MaxLength="10" Nullable="false"/>
        <NavigationProperty Name="employee" Relationship="gdpr.gra.sample.LeaveRequest_employee" FromRole="LeaveRequest" ToRole="Employee"/>
        <Property Name="employee_userId" Type="Edm.String" MaxLength="16" Nullable="false"/>
      </EntityType>

      <Association Name="Address_employee">
        <End Role="Address" Type="gdpr.gra.sample.Address" Multiplicity="*"/>
        <End Role="Employee" Type="gdpr.gra.sample.Employee" Multiplicity="1"/>
        <ReferentialConstraint>
          <Principal Role="Employee">
            <PropertyRef Name="userId"/>
          </Principal>
          <Dependent Role="Address">
            <PropertyRef Name="employee_userId"/>
          </Dependent>
        </ReferentialConstraint>
      </Association>

      <Association Name="LeaveRequest_employee">
        <End Role="LeaveRequest" Type="gdpr.gra.sample.LeaveRequest" Multiplicity="*"/>
        <End Role="Employee" Type="gdpr.gra.sample.Employee" Multiplicity="1"/>
        <ReferentialConstraint>
          <Principal Role="Employee">
            <PropertyRef Name="userId"/>
          </Principal>
          <Dependent Role="LeaveRequest">
            <PropertyRef Name="employee_userId"/>
          </Dependent>
        </ReferentialConstraint>
      </Association>

      <Annotations Target="gdpr.gra.sample.Employee" xmlns="http://docs.oasis-open.org/odata/ns/edm">
        <Annotation Term="Communication.Contact">
          <Record>
            <PropertyValue Property="n">
              <Record>
                <PropertyValue Property="given" Path="firstName"/>
                <PropertyValue Property="additional" Path="middleName"/>
                <PropertyValue Property="surname" Path="lastName"/>
              </Record>
            </PropertyValue>
            <PropertyValue Property="adr" Path="addresses/@Communication.Address"/>
            <PropertyValue Property="email">
              <Collection>
                <Record>
                  <PropertyValue Property="address" Path="email"/>
                </Record>
              </Collection>
            </PropertyValue>
            <PropertyValue Property="gender" Path="genderCode"/>
            <PropertyValue Property="bday" Path="birthDate"/>
          </Record>
        </Annotation>
      </Annotations>

      <Annotations Target="gdpr.gra.sample.EntityContainer/Employee" xmlns="http://docs.oasis-open.org/odata/ns/edm">
        <Annotation Term="PersonalData.EntitySemantics" String="DataSubject"/>
      </Annotations>

      <Annotations Target="gdpr.gra.sample.Employee/userId" xmlns="http://docs.oasis-open.org/odata/ns/edm">
        <Annotation Term="PersonalData.FieldSemantics" String="DataSubjectID"/>
        <Annotation Term="PersonalData.IsUserID"/>
      </Annotations>
      <Annotations Target="gdpr.gra.sample.Employee/email" xmlns="http://docs.oasis-open.org/odata/ns/edm">
        <Annotation Term="Communication.IsEmailAddress" Bool="true"/>
        <Annotation Term="Core.Description" String="Email address, e.g. foo@bar.com"/>
      </Annotations>
      <Annotations Target="gdpr.gra.sample.Employee/genderCode" xmlns="http://docs.oasis-open.org/odata/ns/edm">
        <Annotation Term="PersonalData.IsPotentiallySensitive"/>
      </Annotations>


      <Annotations Target="gdpr.gra.sample.Address" xmlns="http://docs.oasis-open.org/odata/ns/edm">
        <Annotation Term="Communication.Address">
          <Record>
            <PropertyValue Property="country" Path="country"/>
            <PropertyValue Property="region" Path="region"/>
            <PropertyValue Property="locality" Path="city"/>
            <PropertyValue Property="district" Path="district"/>
            <PropertyValue Property="code" Path="streetPostalCode"/>
            <PropertyValue Property="street" Path="streetName"/>
            <PropertyValue Property="ext" Path="houseID"/>
            <PropertyValue Property="careof" Path="careOfName"/>
            <PropertyValue Property="building" Path="buildingID"/>
          </Record>
        </Annotation>
      </Annotations>

      <Annotations Target="gdpr.gra.sample.EntityContainer/Address" xmlns="http://docs.oasis-open.org/odata/ns/edm">
        <Annotation Term="PersonalData.EntitySemantics" String="DataSubjectDetails"/>
      </Annotations>

      <Annotations Target="gdpr.gra.sample.Address/employee_userId" xmlns="http://docs.oasis-open.org/odata/ns/edm">
        <Annotation Term="PersonalData.FieldSemantics" String="DataSubjectID"/>
        <Annotation Term="PersonalData.IsUserID"/>
      </Annotations>


      <Annotations Target="gdpr.gra.sample.EntityContainer/LeaveRequest" xmlns="http://docs.oasis-open.org/odata/ns/edm">
        <Annotation Term="PersonalData.EntitySemantics" String="LegalGround"/>
      </Annotations>

      <Annotations Target="gdpr.gra.sample.LeaveRequest/leaveRequestId" xmlns="http://docs.oasis-open.org/odata/ns/edm">
        <Annotation Term="PersonalData.FieldSemantics" String="LegalGroundID"/>
      </Annotations>
      <Annotations Target="gdpr.gra.sample.LeaveRequest/employee_userId" xmlns="http://docs.oasis-open.org/odata/ns/edm">
        <Annotation Term="PersonalData.FieldSemantics" String="DataSubjectID"/>
        <Annotation Term="PersonalData.IsUserID"/>
      </Annotations>

    </Schema>
  </edmx:DataServices>
</edmx:Edmx>
