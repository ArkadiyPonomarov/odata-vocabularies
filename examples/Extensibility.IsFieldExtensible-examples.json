{
    "$Version": "4.0",
    "$Reference": {
        "https://oasis-tcs.github.io/odata-vocabularies/vocabularies/Org.OData.Core.V1.json": {
            "$Include": [
                {
                    "$Namespace": "Org.OData.Core.V1",
                    "$Alias": "Core"
                }
            ]
        },
        "https://sap.github.io/odata-vocabularies/vocabularies/Extensibility.json": {
            "$Include": [
                {
                    "$Namespace": "com.sap.vocabularies.Extensibility.v1",
                    "$Alias": "Extensibility"
                }
            ]
        }
    },
    "Extensibility.examples": {
        "$Alias": "this",
        "default": {
            "$Kind": "EntityContainer",
            "Foo": {
                "$Collection": true,
                "$Type": "this.Foo"
            }
        },
        "Foo": {
            "$Kind": "EntityType",
            "$Key": [
                "id"
            ],
            "id": {},
            "zz_bar": {
                "$Nullable": true
            }
        },
        "$Annotations": {
            "this.default": {
                "@Extensibility.FieldExtensibilityMechanism": {
                    "DiscoveryUrl": "/get-cap-extension-capabilities"
                }
            },
            "this.Foo": {
                "@Extensibility.Extensible": true,
                "@Extensibility.Enabled": {
                    "Fields": true,
                    "Composition": true,
                    "FieldsFromNewDataSource": true,
                    "Annotations": true
                },
                "@Extensibility.ExtensionPoints": [
                    "sap.someApp:dataModelExtensionPoint:CUSTOMER_PROJECT",
                    "sap.aligned:dataModelExtensionPoint:MAIN_PROJECT"
                ]
            },
            "this.Foo/id": {
                "@Extensibility.Exposable": true
            },
            "this.Foo/zz_bar": {
                "@Extensibility.ExtensionField": true
            }
        }
    },
    "$EntityContainer": "Extensibility.examples.default"
}